{
  "expo": {
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "name": "Word of Mouth",
    "slug": "word_of_mouth",
    "version": "1.0.22",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.png",
    "scheme": "wordofmouth",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "associatedDomains": ["applinks:wordofmouth.vercel.app"],
      "supportsTablet": true,
      "bundleIdentifier": "com.wordofmouth.limited",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDg8mHKCgXmniBiuEkGgcl3Rz5UYArQ67Q"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDg8mHKCgXmniBiuEkGgcl3Rz5UYArQ67Q"
        }
      },
      "package": "com.wordofmouth.limited",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
       [
        "react-native-share",
        {
          "platforms": {
            "ios": [
              "fb",
              "instagram",
              "twitter",
              "tiktoksharesdk"
            ],
            "android": [
              "com.facebook.katana",
              "com.instagram.android",
              "com.twitter.android",
              "com.zhiliaoapp.musically"
            ]
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "WORD OF MOUTH needs camera access to take photos of places you want to share with others."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "WORD OF MOUTH needs photo library access to let you select images to share with your recommendations.",
          "cameraPermission": "WORD OF MOUTH needs camera access to take photos for your recommendations."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "WORD OF MOUTH uses your location to show recommendations near you and to tag locations when you create new recommendations."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#000000",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dca69f70-aefa-4293-bc55-250e5f875a75"
      }
    },
    "updates": {},
    "owner": "rafayyousafzai"
  }
}
