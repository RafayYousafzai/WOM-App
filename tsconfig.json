    "components/post-listing/PostHeader.jsx",
    "components/camera/CameraScreen.jsx",
    "app/(root)/camera.jsx",
    "app/(root)/(tabs)/create-review.tsx",
    "components/create-post/ErrorBoundary.jsx",
    "components/PageSkeletons/ImageWithSkeleton.jsx",
    "app/(auth)/sign-up.jsx",
    "components/create-post/archived-review/Step2DetailsInput.jsx",
    "components/post-listing/PostCard.jsx",
    "components/post-listing/PostContent.jsx",
    "components/common/TagInputWithSuggestions.tsx",
    "components/profile-view/AppleProfileSetup.jsx",
    "components/search-feed/AllResultsList.jsx",
    "context/globalContext.jsx",
    "components/create-post/image-edit/image-preview-editor.tsx",
