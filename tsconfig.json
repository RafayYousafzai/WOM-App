    "components/create-post/image-edit/enhanced-image-editor.jsx",
    "components/common/DietaryOptionTag.tsx",
    "lib/supabase/postUpdateActions.js",
    "components/post-listing/EditPost/EditPostHeader.jsx",
    "components/post-listing/EditPost/EditPostScreen.jsx",
    "app/(root)/edit-post.jsx"
, "components/post-listing/DishSidebar.tsx", "components/post-listing/RestaurantInfo.jsx"  ]
}
